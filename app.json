{
  "expo": {
    "name": "blog-native-app",
    "slug": "blog-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "blognativeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/blog-native-app-icon.png",
        "backgroundImage": "./assets/images/blog-native-app-icon.png",
        "monochromeImage": "./assets/images/blog-native-app-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sidr99.blognativeapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/blog-native-app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/blog-native-app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0d5ee6a-128f-4b09-8312-9c77b323004d"
      }
    }
  }
}
